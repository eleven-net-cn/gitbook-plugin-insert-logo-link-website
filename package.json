{
  "name": "gitbook-plugin-insert-logo-link-website",
  "version": "1.0.2",
  "description": "GitBook Plugin to insert (url or file or base64) into the navigation above search input.",
  "keywords": [
    "gitbook",
    "plugin",
    "insert-logo",
    "logo",
    "link"
  ],
  "main": "index.js",
  "author": "matusnovak,Eleven",
  "readme": "GitBook Plugin to insert logo (url or file or base64) into the navigation above search input.",
  "engines": {
    "gitbook": ">1.x.x"
  },
  "gitbook": {
    "properties": {
      "url": {
        "type": "string",
        "default": "http://www.example.com/my-logo.png",
        "title": "Logo URL"
      },
      "link": {
        "type": "string",
        "default": "javascript:;",
        "title": "websit link"
      },
      "target": {
        "type": "string",
        "default": "_blank",
        "title": "link mode"
      },
      "style": {
        "type": "string",
        "default": "background: none;",
        "title": "Custom CSS"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Eleven90/gitbook-plugin-insert-logo-link-website.git"
  }
}
